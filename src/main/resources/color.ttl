@base  <http://$host:$port/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix frmt: <http://www.w3.org/ns/formats/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix vsd: <http://w3id.org/rsp/vocals-sd#> .
@prefix vprov:  <http://w3id.org/rsp/vocals-prov#> .
@prefix vocals: <http://w3id.org/rsp/vocals#> .

<blue> a vocals:StreamDescriptor .

<$color> a vocals:RDFStream ;
	dcat:title "Blue Color Stream"^^xsd:string ;
	dcat:description "Periodically produces instances of the Blue Color Class"^^xsd:string ;
    dcat:publisher <http://localhost:8181/colorwave> ;
    dcat:landingPage <https://example.org/color/blue/> ;
	vocals:hasEndpoint <BlueColorStreamEndpoint>  .

<$ColorStreamEndpoint> a vocals:StreamEndpoint ;
	dcat:title "Blue Color Stream Endpoint"^^xsd:string ;
	dcat:description "Streaming endpoint to consume Blue color instances via websocket"^^xsd:string ;
	dcat:license <https://creativecommons.org/licenses/by-nc/4.0/> ;
	dcat:format frmt:JSON-LD ;
	dcat:accessURL "ws://localhost:2552/blue" ;
    vsd:publishedBy <http://localhost:8181/colorwave> .

<http://localhost:8181/colorwave> a vsd:PublishingService ;
	vsd:hasFeature vsd:replaying ;
	vsd:resultFormat frmt:JSON-LD .