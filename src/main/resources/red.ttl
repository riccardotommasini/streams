@base  <http://redstream:1255/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix frmt: <http://www.w3.org/ns/formats/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix vsd: <http://w3id.org/rsp/vocals-sd#> .
@prefix vprov:  <http://w3id.org/rsp/vocals-prov#> .
@prefix vocals: <http://w3id.org/rsp/vocals#> .

<red> a vocals:StreamDescriptor .

<red> a vocals:RDFStream ;
	dcat:title "Red Color Stream"^^xsd:string ;
	dcat:description "Periodically produces instances of the Red Color Class"^^xsd:string ;
    dcat:publisher <http://streamhub:9292/streamhub> ;
    dcat:landingPage <https://example.org/color/red/> ;
	vocals:hasEndpoint <RedColorStreamEndpoint>  .

<RedColorStreamEndpoint> a vocals:StreamEndpoint ;
	dcat:title "Red Color Stream Endpoint"^^xsd:string ;
	dcat:description "Streaming endpoint to consume Red color instances via websocket"^^xsd:string ;
	dcat:license <https://creativecommons.org/licenses/by-nc/4.0/> ;
	dcat:format frmt:JSON-LD ;
	dcat:accessURL "ws://redstream:2551/red" ;
    vsd:publishedBy <http://streamhub:9292/streamhub> .

<http://streamhub:9292/streamhub> a vsd:PublishingService ;
	vsd:hasFeature vsd:replaying ;
	vsd:resultFormat frmt:JSON-LD .